	PremiumOnly = false
})
Tab:AddButton({
	Name = "Tools",
	Callback = function()
      	for _,v in pairs(workspace:GetDescendants()) do
	if v:IsA("Tool") then
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Handle.CFrame
	end
end

  	end   
 })
Tab:AddButton({
	Name = "Humanoid",
	Callback = function()
 game.Players.LocalPlayer.Character.Humanoid.Health = 0
  	end    
})
